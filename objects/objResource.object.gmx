//when in contact with gathering man, gets gathered
if place_meeting(x, y, objMan) {
    var gatherer = instance_place(x,y, objMan)
    if gatherer.state == scrStateGathering {
        //gains gathering percent when player is there
        gatherPercent ++
    }
    //once it hits 100%, give player the resource
    if gatherPercent &gt;= 100 {
        with gatherer {
            weight ++
            energy -= 10
            ds_stack_push(held, other.id)
            other.y = y - 32*ds_stack_size(held) - 2
            state = scrStateIdle
            if objGame.selected = id {
                sprite_index = sprManCollectingSelected
            } else {
                sprite_index = sprManCollecting
            }
        }
        //turns into box
        image_index = 1
        player = gatherer
    }
} else {
    //loses gathering percent when they leave, to an extent
    gatherPercent --
    if gatherPercent &lt;= 0 {
        gatherPercent = 0
    }
}

//carried by player
if player != noone {
    x = player.x
}

//drops in bucket
if place_meeting(x, y, objBucket) {
    objGame.resources[type] ++
    instance_destroy()
}

</string>
