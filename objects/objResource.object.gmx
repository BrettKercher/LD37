            <string>
//when in contact with gathering man, gets gathered
if place_meeting(x, y, objMan) {
    var gatherer = instance_place(x,y, objMan)
    if gatherer.state == scrStateGathering {
        //gains gathering percent when player is there
        if((gatherer.tool == AXE and type == WOOD)
            or (gatherer.tool == PICK and type == ROCK)
            or (gatherer.tool == SPEAR and type == FOOD)) {
            gatherPercent += 2;
        }
        else {
            gatherPercent ++;
        }
        
    }
    //once it hits 100%, give player the resource
    if (gatherPercent &gt;= 100) {
        if(!tierTwo)
        {
            with gatherer {
                weight ++
                energy -= 10
                ds_stack_push(held, other.id)
                other.y = y - 32*ds_stack_size(held) - 2
                state = scrStateIdle
                sprite_index = sprManCollecting
                image_speed = 0
                image_index = 0
            }
            //turns into box
            image_index = 1
            player = gatherer
        }
        else
        {
            with gatherer {
                state = scrStateIdle;
                energy -= 10;
            }
            instance_destroy();
            
            scrCreateResource(type, x - 32);
            
            if(type == WOOD)
            {
                scrCreateResource(SAP, x );
            }
            else if(type == ROCK)
            {
                scrCreateResource(GEM, x );
            }
            else if(type == FOOD)
            {
                scrCreateResource(FUR, x );
            }
        }
    }
} else {
    //loses gathering percent when they leave, to an extent
    gatherPercent --
    beingGathered = false
    if gatherPercent &lt;= 0 {
        gatherPercent = 0
    }
}

//carried by player
if player != noone {
    x = player.x
}

//drops in bucket
if place_meeting(x, y, objBucket) {
    objGame.resources[type] ++
    instance_destroy()
}

</string>
          </argument>
        </arguments>
      </action>
