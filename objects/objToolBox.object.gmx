//when in contact with equipping man, gets equipped
if place_meeting(x, y, objMan) {
    var equipper = instance_place(x,y, objMan)
    if equipper.state == scrStateEquipping {
        //gains equip percent when player is there
        equipPercent ++;
    }   
} else {
    equipPercent = 0
    beingEquipped = false
}

//once it hits 100%, give player the resource
if (equipPercent &gt;= fullEquip) and beingEquipped = true {
    if equipper != noone and objGame.stored[toolType] &gt; 0 and equipper.tool == noone {
        objGame.stored[toolType]--;
        objGame.held[toolType]++
        image_index = clamp(objGame.stored[toolType], 0, 2);
        equipper.tool = toolType;
    }
    else if(equipper != noone and equipper.tool == toolType) {
        objGame.stored[toolType]++;
        objGame.held[equipper.tool]--
        image_index = clamp(objGame.stored[toolType], 0, 2);
        equipper.tool = noone;
    }
    with equipper {
        state = scrStateIdle;
        sprite_index = sprMan
        image_speed = 0
        image_index = 0
    }
    equipPercent = 0
    beingEquipped = false;
}
</string>
